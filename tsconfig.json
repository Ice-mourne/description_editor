{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": false,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",

    "baseUrl": ".",
    "paths": {
      "@ItemPopup": ["src/components/itemPopup/exports.ts"],
      "@SideBar": ["src/components/sideBar/exports.ts"],



      "@styles/*": ["src/component_styles/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@ts/*": ["src/ts/*"],
    }
  },
  "include": ["./src"]
}
